   "source": [
    "arr1=[]\n",
    "def reccomend(arr):\n",
    "  arr1.clear()\n",
    "  for i in range(0, len(arr)):\n",
    "    arr2=[]\n",
    "    count=0\n",
    "    for j in range(0, len(arr[i])):\n",
    "      if(arr[i][j]>0.5):\n",
    "        count=1\n",
    "        arr2.append(j)\n",
    "    if(count==0):\n",
    "      num=max(arr[i])\n",
    "      for j in range(0,len(arr[i])):\n",
    "        if(arr[i][j]==num):\n",
    "          arr2.append(j)\n",
    "    arr1.append(arr2)"
   ]
  },
